{
  "manifest_version": 3,
  "name": "Trimaxion",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "contextMenus"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["search.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  }
}
